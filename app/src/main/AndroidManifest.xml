<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="assignment.mobile.playmp3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Dark">

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" />
        <activity android:name=".PlayingActivity" />
        <activity android:name=".MenuHomeActivity" android:noHistory="true" />
        <activity android:name=".MenuSearchActivity" android:noHistory="true" />
        <activity android:name=".MenuLibraryActivity" android:noHistory="true" />
        <activity android:name=".MenuUploadActivity" android:noHistory="true" />
    </application>

</manifest>